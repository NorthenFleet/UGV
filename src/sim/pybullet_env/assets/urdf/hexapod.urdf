<?xml version="1.0"?>
<robot name="hexapod">
  <link name="base">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="6.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.35 0.25 0.08"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.35 0.25 0.08"/>
      </geometry>
    </collision>
  </link>

  <link name="LF_coxa_link">
    <inertial>
      <mass value="0.25"/>
      <inertia ixx="8e-4" iyy="8e-4" izz="8e-4"/>
    </inertial>
    <visual>
      <origin xyz="0.045 0 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.09" radius="0.02"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.045 0 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.09" radius="0.02"/>
      </geometry>
    </collision>
  </link>

  <link name="LF_femur_link">
    <inertial>
      <mass value="0.20"/>
      <inertia ixx="6e-4" iyy="6e-4" izz="6e-4"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.02"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.02"/>
      </geometry>
    </collision>
  </link>

  <link name="LF_tibia_link">
    <inertial>
      <mass value="0.18"/>
      <inertia ixx="5e-4" iyy="5e-4" izz="5e-4"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.08" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.16" radius="0.02"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.08" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.16" radius="0.02"/>
      </geometry>
    </collision>
  </link>

  <joint name="LF_COXA" type="revolute">
    <parent link="base"/>
    <child  link="LF_coxa_link"/>
    <origin xyz="0.17 0.14 0.02" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.5" upper="1.5" effort="5" velocity="4.0"/>
  </joint>

  <joint name="LF_FEMUR" type="revolute">
    <parent link="LF_coxa_link"/>
    <child  link="LF_femur_link"/>
    <origin xyz="0.09 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="0.7" effort="5" velocity="4.0"/>
  </joint>

  <joint name="LF_TIBIA" type="revolute">
    <parent link="LF_femur_link"/>
    <child  link="LF_tibia_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="0.5" effort="5" velocity="4.0"/>
  </joint>

</robot>
